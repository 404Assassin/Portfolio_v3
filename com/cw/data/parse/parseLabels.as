package com.cw.data.parse{	/*	-=] label based parser [=-	-=] language version ActionScript 3.0 [=-	-=] player version Flash 10.0 [=-	-=] author Christian Worley [=-	-=] created 04/2011 [=-	*/		//[=-------------------------------------------------	// Imports	//[=-------------------------------------------------	import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.events.Event;	import flash.net.URLLoader;	//[=-------------------------------------------------	// Constructor	//[=-------------------------------------------------	public class parseLabels {		//[=-------------------------------------------------		// Public Vars		//[=-------------------------------------------------		public var publicVar:String;				public var _name:String;		//[=-------------------------------------------------		// Private Vars		//[=-------------------------------------------------		private var privateVar:String;		//[=-------------------------------------------------		// Public Methods		//[=-------------------------------------------------		//-=] Method discription [=-\\		public function publicClassMethod () {			trace ('publicClassMethod');			loadTheXML ();		}		//-=] Method discription [=-\\		public function loadTheXML () {			var loader:URLLoader = new URLLoader();			loader.addEventListener(Event.COMPLETE, procesXML);			loader.load(new URLRequest('xml/content.xml'));		}		private function procesXML(loadComplete:Event){			var theXML:XML;			theXML = new XML(loadComplete.target.data);			var stringMeAlong:String = theXML.toString()			trace(stringMeAlong);		}					//[=-------------------------------------------------		// Private Methods		//[=-------------------------------------------------		//-=] Method discription [=-\\		private function privateClassMethod () {			trace ('privateClassMethod');		}		//[=-------------------------------------------------		// Getter/Setters		//[=-------------------------------------------------		//the private class variable is _name.		public function get name():String {    		return _name;  		}		public function set name(value:String):void {    		_name = value;  		}	}}