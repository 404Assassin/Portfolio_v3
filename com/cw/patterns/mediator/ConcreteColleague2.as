package com.cw.patterns.mediator{	public class ConcreteColleague2 extends Colleague{		private var light:String;		private var colObj:Object;		private var om:Mediator;		function ConcreteColleague2(){			trace("__Concrete Colleague #2");			colObj=new Object  ();		}		public override function listenMediator(m:Mediator):void{			trace("__Concrete Colleague #2 listenMediator");			colObj.om=m;		}		public override function doChange(light:String,cn:String):void{			trace("__Concrete Colleague #2 doChange");			this.light=light;			colObj.om.update(light,cn);		}	}}