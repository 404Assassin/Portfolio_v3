package com.cw.patterns.mediator{	public class ConcreteColleague1 extends Colleague{		private var light:String;		private var colObj:Object;		private var om:Mediator;		function ConcreteColleague1(){			trace("__Concrete Colleague #1");			colObj=new Object  ();		}		public override function listenMediator(m:Mediator):void{			trace("__Concrete Colleague #1 listenMediator");			colObj.om=m;		}		public override function doChange(light:String,cn:String):void{			trace("__Concrete Colleague #1 doChange");			this.light=light;			colObj.om.update(light,cn);		}	}}